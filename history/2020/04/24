              <a class="text-gray-dark" href="https://github.com/pcottle/learnGitBranching/issues/638" data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;REPOSITORY_CARD&quot;,&quot;click_target&quot;:&quot;ISSUE&quot;,&quot;click_visual_representation&quot;:&quot;BIG_GOOD_FIRST_ISSUE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:565950068,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="80ba9f6fd1fad35e257c40178b041015b3514f36fe4f45900dfaf9d14b6af026" data-ga-click="Explore, go to issue, location:good first issues;size:large;">
