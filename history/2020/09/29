      <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;REPOSITORY_CARD&quot;,&quot;click_target&quot;:&quot;REPOSITORY_PULL_REQUESTS&quot;,&quot;click_visual_representation&quot;:&quot;PULL_REQUESTS_TAB&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:267820792,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="7ee274b4866ec79c72c91dd81b2f243a0686d5baac7b4dfb7384aa714c32b6a2" data-ga-click="Explore, go to repository pulls, location:explore feed" href="/deepnight/led/pulls" class="tabnav-tab f6 px-2 py-1">
