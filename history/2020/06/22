          <a class="CircleBadge CircleBadge--small " href="/marketplace/codefresh" data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;MARKETPLACE_APP_CARD&quot;,&quot;click_target&quot;:&quot;MARKETPLACE_APP&quot;,&quot;click_visual_representation&quot;:&quot;MARKETPLACE_APP_ICON&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:324,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="dbff32a8a3d7ec3b4ecc9c80e4c38ab9105920380a474b744dcb4d1fa7ee9cba" data-ga-click="Explore, go to marketplace app, location:explore feed" style="background-color: #19b99a;">
