        <div class="col-4 col-sm-3 mx-auto">
          <a href="/collections/text-editors" data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;COLLECTION_CARD&quot;,&quot;click_target&quot;:&quot;COLLECTION&quot;,&quot;click_visual_representation&quot;:&quot;COLLECTION_IMAGE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:29,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="3f8ccf020dbf26b73a9695264a90fbecce1dd4bc8ca84db3c96394052491ea1f" data-ga-click="Explore, go to collection, location:explore feed; text:Text editors">
