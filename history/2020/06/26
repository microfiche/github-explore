          <a class="CircleBadge CircleBadge--small " href="/marketplace/lucidchart-connector" data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;MARKETPLACE_APP_CARD&quot;,&quot;click_target&quot;:&quot;MARKETPLACE_APP&quot;,&quot;click_visual_representation&quot;:&quot;MARKETPLACE_APP_ICON&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:3112,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="a688b3db3bf271f984faa48a19e89ac2adb036a89a302446744baf8fde680c64" data-ga-click="Explore, go to marketplace app, location:explore feed" style="background-color: #F96B13;">
