            <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;COLLECTION_CARD&quot;,&quot;click_target&quot;:&quot;COLLECTION&quot;,&quot;click_visual_representation&quot;:&quot;COLLECTION_TITLE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:29,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="ef8f54bea4a3c6866b72d51dc700357b5ed6fdd034cb69c9a9441b86005c67cc" data-ga-click="Explore, go to collection, location:explore feed; text:Text editors" href="/collections/text-editors">
