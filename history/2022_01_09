      <div class="d-flex">
        <div class="mr-4 ml-2">
          <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;MARKETPLACE_APP_CARD&quot;,&quot;click_target&quot;:&quot;MARKETPLACE_APP&quot;,&quot;click_visual_representation&quot;:&quot;MARKETPLACE_APP_ICON&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:1110,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="3d65b8b1d734e4b119b341d5f87d19b675e369ce397e4f03b67fecfb3cb2afbe" data-ga-click="Explore, go to marketplace app, location:explore feed" style="background-color: #ffffff;" href="/marketplace/stale" data-view-component="true" class="CircleBadge CircleBadge--small">
            <img
