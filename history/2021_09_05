            <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;MARKETPLACE_APP_CARD&quot;,&quot;click_target&quot;:&quot;MARKETPLACE_APP&quot;,&quot;click_visual_representation&quot;:&quot;MARKETPLACE_APP_TITLE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:4875,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="e93d064890a4b7efe4e5b909574b2880955ea1ed42e45a59e337bbebe422226b" data-ga-click="Explore, go to marketplace app, location:explore feed; text:Create Issue Branch" href="/marketplace/create-issue-branch" data-view-component="true">
              Create Issue Branch
</a>          </h3>
          <div class="markdown-body f5 color-text-secondary mb-2">
