              <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;REPOSITORY_CARD&quot;,&quot;click_target&quot;:&quot;ISSUE&quot;,&quot;click_visual_representation&quot;:&quot;BIG_GOOD_FIRST_ISSUE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:313913149,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="1cb14cdbc16ca70692c80f48234c3bedc58982261494fd028f458371d38cb302" data-ga-click="Explore, go to issue, location:good first issues;size:large;" href="https://github.com/OpenZeppelin/openzeppelin-contracts/issues/890" data-view-component="true" class="color-text-primary">
                Add ECDSA#toEthSignedMessageHash for bytes type
</a>            </h4>
          </div>

            <div class="f6 color-text-secondary text-right no-wrap mt-1 ml-3">
              <svg aria-hidden="true" viewBox="0 0 16 16" version="1.1" data-view-component="true" height="16" width="16" class="octicon octicon-comment color-text-secondary mr-1">
    <path fill-rule="evenodd" d="M2.75 2.5a.25.25 0 00-.25.25v7.5c0 .138.112.25.25.25h2a.75.75 0 01.75.75v2.19l2.72-2.72a.75.75 0 01.53-.22h4.5a.25.25 0 00.25-.25v-7.5a.25.25 0 00-.25-.25H2.75zM1 2.75C1 1.784 1.784 1 2.75 1h10.5c.966 0 1.75.784 1.75 1.75v7.5A1.75 1.75 0 0113.25 12H9.06l-2.573 2.573A1.457 1.457 0 014 13.543V12H2.75A1.75 1.75 0 011 10.25v-7.5z"></path>
