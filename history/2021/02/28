              <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;REPOSITORY_CARD&quot;,&quot;click_target&quot;:&quot;ISSUE&quot;,&quot;click_visual_representation&quot;:&quot;BIG_GOOD_FIRST_ISSUE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:484812953,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="f6c2553abe04ea41d7cc3abb26173996294804273bf61dfa2b235a53eec04325" data-ga-click="Explore, go to issue, location:good first issues;size:large;" href="https://github.com/google/googletest/issues/2429" class="text-gray-dark">
