              <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;REPOSITORY_CARD&quot;,&quot;click_target&quot;:&quot;ISSUE&quot;,&quot;click_visual_representation&quot;:&quot;BIG_GOOD_FIRST_ISSUE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:807315644,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="31c29ccfd964d8d6114e1a5094e208fb1afab410b3967acb1ae8e12e97724fda" data-ga-click="Explore, go to issue, location:good first issues;size:large;" href="https://github.com/catchorg/Catch2/issues/2175" data-view-component="true" class="color-text-primary">
                Namespace tests with filename to reduce test naming conflicts
</a>            </h4>
          </div>

            <div class="f6 color-text-secondary text-right no-wrap mt-1 ml-3">
              <svg aria-hidden="true" height="16" viewBox="0 0 16 16" version="1.1" width="16" data-view-component="true" class="octicon octicon-comment color-text-secondary mr-1">
