          <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;COLLECTION_CARD&quot;,&quot;click_target&quot;:&quot;COLLECTION&quot;,&quot;click_visual_representation&quot;:&quot;COLLECTION_IMAGE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:39,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="f0a3de23edd31b72dce2e1b204226bb59ffcb340dd9b66d11f02008141e02ecf" data-ga-click="Explore, go to collection, location:explore feed; text:Game Engines" href="/collections/game-engines" data-view-component="true">
