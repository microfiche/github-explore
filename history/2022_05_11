<a id="label-f71d2d" href="/issues?q=is%3Aopen+is%3Aissue+label%3A%22good+first+issue%22" data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;CONTRIBUTE_PAGE&quot;,&quot;click_target&quot;:&quot;LABEL_SCOPED_ISSUE_SEARCH&quot;,&quot;click_visual_representation&quot;:&quot;LABEL_TAG&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:795985873,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="fe66aa64dac82240aebd85aef2cc2a142d0c4e6c9195a9ec36d39a15bb1f4f40" data-turbo="false" data-name="good first issue" style="--label-r:112;--label-g:87;--label-b:255;--label-h:248;--label-s:100;--label-l:67;" data-view-component="true" class="IssueLabel hx_IssueLabel">
                    good first issue
