          <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;TOPIC_RECOMMENDATION_CARD&quot;,&quot;click_target&quot;:&quot;TOPIC&quot;,&quot;click_visual_representation&quot;:&quot;TOPIC_TITLE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:204,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="d3f8313676cf1595b1ccfec1e4f6c8f5d94679965e63e34e4623365ee4871be0" data-ga-click="Explore, go to topic, location:explore feed; text:Jekyll" href="/topics/jekyll" data-view-component="true">
