            <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;COLLECTION_CARD&quot;,&quot;click_target&quot;:&quot;COLLECTION&quot;,&quot;click_visual_representation&quot;:&quot;COLLECTION_TITLE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:14,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="f30ab7e34bcd28f028587f65c06248d89da340e09daba766fea6c3e05b4044b6" data-ga-click="Explore, go to collection, location:explore feed; text:Learn to Code" href="/collections/learn-to-code" data-view-component="true">
                Learn to Code
