                <h4 class="d-inline flex-auto h5 lh-condensed">
                  <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;REPOSITORY_CARD&quot;,&quot;click_target&quot;:&quot;ISSUE&quot;,&quot;click_visual_representation&quot;:&quot;LITTLE_GOOD_FIRST_ISSUE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:1065308868,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="361202ce89de9ba326201f4f166727ec7a6b36b6b92e7cf8867616c7c0c1e88c" data-ga-click="Explore, go to issue, location:good first issues;size:small;" data-turbo="false" href="https://github.com/rust-lang/rust-analyzer/issues/10881" data-view-component="true" class="color-fg-default">
  
