          <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;TOPIC_RECOMMENDATION_CARD&quot;,&quot;click_target&quot;:&quot;TOPIC&quot;,&quot;click_visual_representation&quot;:&quot;TOPIC_TITLE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:598,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="9613ba7a53e9ded5748ccc389650663ff92777f376b07a6711a6bc9fb0ca5b88" data-ga-click="Explore, go to topic, location:explore feed; text:npm" href="/topics/npm" data-view-component="true">
  
            npm

  
