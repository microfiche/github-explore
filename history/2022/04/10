  <article class="border rounded color-shadow-small color-bg-subtle my-4">
      <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;TOPIC_RECOMMENDATION_CARD&quot;,&quot;click_target&quot;:&quot;TOPIC&quot;,&quot;click_visual_representation&quot;:&quot;TOPIC_LOGO&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:621,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="c88a2e4b7259e86a1bd49cddfbce0d5c7359e3eea9e18e243d333679daa67969" data-ga-click="Explore, go to topic, location:explore feed; text:Amazon Web Services" href="/topics/aws" data-view-component="true" class="overflow-hidden rounded-top-2 border-bottom d-block position-relative">
  
        <div
          class="position-absolute top-0 right-0 bottom-0 left-0"
          style="background-image: url(https://raw.githubusercontent.com/github/explore/fbceb94436312b6dacde68d122a5b9c7d11f9524/topics/aws/aws.png); filter: blur(55px); -webkit-filter: blur(55px);"
        ></div>
        <img
          loading="lazy"
          src="https://raw.githubusercontent.com/github/explore/fbceb94436312b6dacde68d122a5b9c7d11f9524/topics/aws/aws.png"
