<!--// Topic //-->


  <article class="border rounded color-shadow-small color-bg-subtle my-4">
      <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;TOPIC_RECOMMENDATION_CARD&quot;,&quot;click_target&quot;:&quot;TOPIC&quot;,&quot;click_visual_representation&quot;:&quot;TOPIC_LOGO&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:22583,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="f3c290ec514f8764b005fd23a0acdd52136ed787a2e5956a705432c37bdde912" data-ga-click="Explore, go to topic, location:explore feed; text:MvvmCross" href="/topics/mvvmcross" data-view-component="true" class="overflow-hidden rounded-top-2 border-bottom d-block position-relative">
  
        <div
          class="position-absolute top-0 right-0 bottom-0 left-0"
          style="background-image: url(https://raw.githubusercontent.com/github/explore/80688e429a7d4ef2fca1e82350fe8e3517d3494d/topics/mvvmcross/mvvmcross.png); filter: blur(55px); -webkit-filter: blur(55px);"
        ></div>
        <img
          loading="lazy"
