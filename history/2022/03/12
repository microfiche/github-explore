              <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;REPOSITORY_CARD&quot;,&quot;click_target&quot;:&quot;ISSUE&quot;,&quot;click_visual_representation&quot;:&quot;BIG_GOOD_FIRST_ISSUE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:1140210232,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="1a8cda01eb107f1baac8e54e63bd27d79f0549aee88dfb2f1def6b43d9ce1506" data-ga-click="Explore, go to issue, location:good first issues;size:large;" href="https://github.com/cypress-io/cypress/issues/20222" data-view-component="true" class="color-fg-default">
                Search is disabled in browser runner window
