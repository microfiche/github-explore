            <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;MARKETPLACE_APP_CARD&quot;,&quot;click_target&quot;:&quot;MARKETPLACE_APP&quot;,&quot;click_visual_representation&quot;:&quot;MARKETPLACE_APP_TITLE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:437,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="a75121bde92aaeaf129d05f0fd5d64ee486286f9689860615012a001be66daf7" data-ga-click="Explore, go to marketplace app, location:explore feed; text:PullRequest" href="/marketplace/pullrequest" data-view-component="true">
              PullRequest
</a>          </h3>
          <div class="markdown-body f5 color-fg-muted mb-2">
