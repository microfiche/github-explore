<a id="label-aabe4d" href="/issues?q=is%3Aopen+is%3Aissue+label%3A%22help+wanted%22" data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;CONTRIBUTE_PAGE&quot;,&quot;click_target&quot;:&quot;LABEL_SCOPED_ISSUE_SEARCH&quot;,&quot;click_visual_representation&quot;:&quot;LABEL_TAG&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:571226465,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="bed07399099f8e2d1e1d0e38f29ba4d19b36225284d7d19bcd76340c55689017" data-turbo="false" data-name="help wanted" style="--label-r:229;--label-g:66;--label-b:116;--label-h:341;--label-s:75;--label-l:57;" data-view-component="true" class="IssueLabel hx_IssueLabel">
