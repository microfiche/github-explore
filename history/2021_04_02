        <div class="mr-4 ml-2">
          <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;MARKETPLACE_APP_CARD&quot;,&quot;click_target&quot;:&quot;MARKETPLACE_APP&quot;,&quot;click_visual_representation&quot;:&quot;MARKETPLACE_APP_ICON&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:497,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="226805f9f4fd3745f92b9c3aab12f631d8d0df563f4a4062ffa87d189cbbf147" data-ga-click="Explore, go to marketplace app, location:explore feed" style="background-color: #438977;" href="/marketplace/imgbot" class="CircleBadge CircleBadge--small ">
            <img
              src="https://avatars.githubusercontent.com/ml/497?s=400&amp;v=4"
              alt="Imgbot"
              class="CircleBadge-icon"
