          <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;COLLECTION_CARD&quot;,&quot;click_target&quot;:&quot;COLLECTION&quot;,&quot;click_visual_representation&quot;:&quot;COLLECTION_IMAGE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:39,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="0b62fcc2cdbc081ec44024998386d598940ad58556853301f286783993326486" data-ga-click="Explore, go to collection, location:explore feed; text:Game Engines" href="/collections/game-engines" data-view-component="true" class="Link"><div
