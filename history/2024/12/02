        <div class="mr-4 ml-2">
          <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;MARKETPLACE_APP_CARD&quot;,&quot;click_target&quot;:&quot;MARKETPLACE_APP&quot;,&quot;click_visual_representation&quot;:&quot;MARKETPLACE_APP_ICON&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:1150,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="64ece2ee880b0d44b1fa22efea68e9a156b81c1efa53732f67eca37f6a728ef7" data-ga-click="Explore, go to marketplace app, location:explore feed" style="background-color: #ffffff;" href="/marketplace/google-cloud-build" data-view-component="true" class="CircleBadge CircleBadge--small Link">
