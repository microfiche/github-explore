          <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;EVENT_CARD&quot;,&quot;click_target&quot;:&quot;EVENT_URL&quot;,&quot;click_visual_representation&quot;:&quot;EVENT_TITLE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:null,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="6e2ee4b8c8419ff9f9547c92bfe6409bc7495ede7e87a235545d987054dbfe8f" data-ga-click="Explore, go to event, location:explore feed; text:LittleJS Game Jam" href="https://itch.io/jam/littlejs-game-jam" data-view-component="true" class="Link">
