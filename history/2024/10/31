      <div class="d-flex">
        <div class="mr-4 ml-2">
          <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;MARKETPLACE_APP_CARD&quot;,&quot;click_target&quot;:&quot;MARKETPLACE_APP&quot;,&quot;click_visual_representation&quot;:&quot;MARKETPLACE_APP_ICON&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:2383,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="09067483abc17b047de3514f26426b378ddc4274fe3900af31f19810c26e2863" data-ga-click="Explore, go to marketplace app, location:explore feed" style="background-color: #000000;" href="/marketplace/datree" data-view-component="true" class="CircleBadge CircleBadge--small Link">
            <img
              src="https://avatars.githubusercontent.com/ml/2383?s=400&amp;v=4"
