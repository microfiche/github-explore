      <div class="d-flex">
        <div class="mr-4 ml-2">
          <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;MARKETPLACE_APP_CARD&quot;,&quot;click_target&quot;:&quot;MARKETPLACE_APP&quot;,&quot;click_visual_representation&quot;:&quot;MARKETPLACE_APP_ICON&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:3591,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="e88181554c0b8cb6c6290549868a5a8c61436108bf7515977ed58f7e98111829" data-ga-click="Explore, go to marketplace app, location:explore feed" style="background-color: #193156;" href="/marketplace/flaptastic" data-view-component="true" class="CircleBadge CircleBadge--small Link"><img
