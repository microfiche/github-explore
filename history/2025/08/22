            <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;COLLECTION_CARD&quot;,&quot;click_target&quot;:&quot;COLLECTION&quot;,&quot;click_visual_representation&quot;:&quot;COLLECTION_TITLE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:14,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="e45cfcaefed16c34bcf609f6e0b540088d03a56b42b82e4047d8287ca49157bb" data-ga-click="Explore, go to collection, location:explore feed; text:Learn to Code" href="/collections/learn-to-code" data-view-component="true" class="Link">Learn to Code</a>          </h3>
