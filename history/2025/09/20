  <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;SPOTLIGHT_CARD&quot;,&quot;click_target&quot;:&quot;SPOTLIGHT_URL&quot;,&quot;click_visual_representation&quot;:&quot;SPOTLIGHT_IMAGE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:null,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="f8ba0a157d287abf524552cb7afb1cd06141b34cb2dfc249cbeb480237efd057" data-ga-click="Explore, go to spotlight, location:explore feed" href="https://github.com/FedeCarollo/jukebox-cli" data-view-component="true" class="rounded-top-2 overflow-hidden Link d-block"><img
