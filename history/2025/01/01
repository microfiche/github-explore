          <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;COLLECTION_CARD&quot;,&quot;click_target&quot;:&quot;COLLECTION&quot;,&quot;click_visual_representation&quot;:&quot;COLLECTION_IMAGE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:33,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="6ee87fa733210f37fe2952c12b21170318a1a2d12e555319f19b79ed46d9d747" data-ga-click="Explore, go to collection, location:explore feed; text:Pixel Art Tools" href="/collections/pixel-art-tools" data-view-component="true" class="Link"><img
