          <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;MARKETPLACE_APP_CARD&quot;,&quot;click_target&quot;:&quot;MARKETPLACE_APP&quot;,&quot;click_visual_representation&quot;:&quot;MARKETPLACE_APP_ICON&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:491,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="fc0255f8e3fae712ef1b2cbb1544ae43cc324a6c2b9046152651bae16eff3494" data-ga-click="Explore, go to marketplace app, location:explore feed" style="background-color: #ffffff;" href="/marketplace/cloud-66-skycap" data-view-component="true" class="CircleBadge CircleBadge--small Link"><img
